#include<iostream>
#include<stdio.h>
#include<vector>
#include<string>
#include<cstdlib>
#include<cstdlib>
#include<stdlib.h>
#include<algorithm>
#include<queue>
#include<math.h>
#include<stack>
#include<set>
#include<map>
#include<sstream>
#include<fstream>
using namespace std;
int main()
{
   string a;
   while(cin>>a)
   {
       int tam=a.length();
       for(int i=0;i<tam;i++)
       {
           char o=a[i];
           o-=7;
           cout<<(char)o;
       }
       cout<<endl;
   }
}
