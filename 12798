#include<bits/stdc++.h>
using namespace std;
vector<long long> v;
int main()
{
	long long n,m,a;
	while(cin>>n>>m)
	{
		v.clear();
		v.resize(n,1);
		long long sol=0;
		for(int i=0;i<n;i++)
		{
			long long sw=1;
			//long long sol=0;
			for(int j=0;j<m;j++)
			{
				cin>>a;
				if(a==0)sw=0;
			}
			if(sw)sol++;
		}
		cout<<sol<<endl;
	}
}
