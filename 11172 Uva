#include<iostream>
#include<stdio.h>
#include<vector>
#include<string>
#include<cstdlib>
#include<cstdlib>
#include<stdlib.h>
#include<algorithm>
#include<queue>
#include<math.h>
#include<stack>
#include<set>
#include<map>
#include<sstream>
#include<fstream>
using namespace std;
int main()
{
   int t;
   cin>>t;
   while(t--)
   {
       long long a,b;
       cin>>a>>b;
       if(a>b)
       {
           cout<<">"<<endl;
       }
       else
       {
           if(a<b)
           {
               cout<<"<"<<endl;
           }
           else
           {
               cout<<"="<<endl;
           }
       }
   }
}
